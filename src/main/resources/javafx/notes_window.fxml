                  <Button mnemonicParsing="false" onMouseClicked="#addNote" text="%add">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button mnemonicParsing="false" onMouseClicked="#removeNote" text="%remove">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
